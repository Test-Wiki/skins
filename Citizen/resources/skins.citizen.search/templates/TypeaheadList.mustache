<div 
	class="citizen-typeahead-group{{#class}} {{.}}{{/class}}"
	id="citizen-typeahead-group-{{type}}"
	data-mw-typeahead-type={{type}}
	{{#keyboardNavigation}}data-mw-typeahead-keyboard-navigation{{/keyboardNavigation}}
	{{#historyValue}}data-mw-typeahead-history-value={{.}}{{/historyValue}}
	{{#hidden}}hidden{{/hidden}}
>
	<ul 
		class="citizen-typeahead-list"
		id="citizen-typeahead-list-{{type}}"
		role="listbox"
	>
		{{#array-list-items}}{{>TypeaheadListItem}}{{/array-list-items}}
	</ul>
</div>